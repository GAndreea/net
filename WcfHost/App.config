<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.serviceModel>
    
    <services>
      <service name="WcfService2.Service1" behaviorConfiguration="myBehavior">
        <endpoint address="WcfService" binding="basicHttpBinding" contract="WcfService2.IService1"></endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8282/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="myBehavior">
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>